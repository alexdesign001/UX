{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "**/PresentationTheme.Aero.csproj",
            "**/PresentationTheme.*.Win10.csproj",
            "**/PresentationTheme.*.Win8.csproj"
          ],
          "exclude": [
            "**/obj/**",
            "**/bin/**"
          ],
          "src": "../Source"
        }
      ],
      "dest": "obj/api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [ "**/*.yml", "index.md" ],
        "src": "obj/api",
        "dest": "api"
      },
      {
        "files": [
          "docs/**.md",
          "docs/**/toc.yml",
          "toc.yml",
          "*.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      },
      {
        "files": "CHANGELOG.md",
        "src": ".."
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "overwrite": "apidoc/**.md",
    "xrefservice": [
      "https://xref.docs.microsoft.com/query?uid={uid}"
    ],
    "dest": "_site",
    "globalMetadata": {
      "_appTitle": "PresentationTheme Aero",
      "_appFooter": "Generated by <strong>DocFX</strong>",
      "_gitContribute": {
        "branch": "master",
        "apiSpecFolder": "Docs/apidoc",
      }
    },
    "fileMetadataFiles": [],
    "template": [
      "default",
      "template"
    ],
    "postProcessors": [],
    "noLangKeyword": false
  }
}
